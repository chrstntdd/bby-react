{
  "name": "bby-react",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Christian Todd <christiantodd27@icloud.com>",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/chrstntdd/bby-react.git"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "lodash.debounce": "^4.0.8",
    "lodash.orderby": "^4.6.0",
    "lodash.shuffle": "^4.2.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-table": "^6.8.0",
    "redux": "^3.7.2",
    "redux-form": "7.0.0",
    "redux-persist": "^4.8.2",
    "redux-thunk": "^2.2.0",
    "url-search-params-polyfill": "^2.0.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^22.2.2",
    "@types/react": "^16.1.0",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/react-router": "^4.0.23",
    "@types/react-router-dom": "^4.2.6",
    "autoprefixer": "^8.2.0",
    "axios-mock-adapter": "^1.14.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "core-js": "^2.5.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "fuse-box": "^3.2.0-next.12",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "jsdom": "^11.6.2",
    "node-sass": "^4.8.3",
    "postcss-flexbugs-fixes": "^3.3.0",
    "purgecss": "^0.21.0",
    "react-router-test-context": "^0.1.0",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.5.1",
    "tailwindcss": "^0.5.1",
    "ts-jest": "^22.4.2",
    "typescript": "^2.8.1",
    "uglify-js": "^3.3.16"
  },
  "scripts": {
    "start": "node fuse dev",
    "prod": "node fuse prod",
    "test": "$(npm bin)/jest",
    "test-auto": "$(npm bin)/jest --watch",
    "prod-test": "NODE_ENV=prod-test npm run prod && node config/mock-server.js",
    "win:prod-test": "set NODE_ENV=prod-test && npm run prod && node config/mock-server.js"
  },
  "babel": {
    "presets": ["env"],
    "plugins": ["transform-es2015-modules-commonjs"]
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "collectCoverageFrom": ["src/**/*.{js,ts,tsx,jsx}"],
    "coverageReporters": ["lcov"],
    "coveragePathIgnorePatterns": ["\\.d\\.ts$"],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]",
      "\\.d\\.ts$",
      "__snapshots__"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.scss$": "<rootDir>/config/mocks/styleMock.js",
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/config/mocks/fileMock.js",
      "^.+\\.(scss|css)$": "<rootDir>/config/mocks/styleMock.js"
    },
    "setupFiles": ["<rootDir>/config/enzyme-setup.js", "<rootDir>/config/jsdom-setup.js"],
    "snapshotSerializers": ["enzyme-to-json/serializer"],
    "testRegex": "\\.spec\\.(js|ts|jsx|tsx)$"
  }
}
