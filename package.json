  "license": "GNU GPLv3",
  "dependencies": {
    "axios": "^0.16.2",
    "lodash.debounce": "^4.0.8",
    "lodash.find": "^4.6.0",
    "lodash.orderby": "^4.6.0",
    "lodash.shuffle": "^4.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-table": "^6.5.1",
    "redux": "^3.7.2",
    "redux-form": "7.0.0",
    "redux-persist": "^4.8.2",
    "redux-thunk": "^2.2.0",
    "url-search-params-polyfill": "^2.0.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^22.2.0",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "@types/react-router": "^4.0.23",
    "@types/react-router-dom": "^4.2.5",
    "@types/redux": "^3.6.0",
    "autoprefixer": "^7.1.1",
    "axios-mock-adapter": "^1.9.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "fuse-box": "^3.1.3",
    "jest": "^23.0.0-alpha.0",
    "node-sass": "^4.5.3",
    "postcss-flexibility": "^2.0.0",
    "react-router-test-context": "^0.1.0",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.2.3",
    "ts-jest": "^22.4.1",
    "typescript": "^2.7.2",
    "uglify-js": "^3.3.15"
  },
  "scripts": {
    "start": "node fuse",
    "dist": "node fuse dist",
    "test": "jest"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "verbose": true,
    "collectCoverage": true,
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "collectCoverageFrom": ["src/**/*.{js,ts,tsx,jsx}"],
    "coverageReporters": ["lcov"],
    "coveragePathIgnorePatterns": ["\\.d\\.ts$"],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]",
      "\\.d\\.ts$",
      "__snapshots__"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.scss$": "<rootDir>/config/mocks/styleMock.js",
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/config/mocks/fileMock.js",
      "^.+\\.(scss|css)$": "<rootDir>/config/mocks/styleMock.js"
    },
    "setupFiles": ["<rootDir>/config/enzyme-setup.js"],
    "snapshotSerializers": ["enzyme-to-json/serializer"],
    "testRegex": "\\.spec\\.(js|ts)x$"
  }
}
