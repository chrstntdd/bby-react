{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"]
    },
    "lib": ["es7", "dom"],
    "sourceMap": true,
    "allowJs": true,
    "module": "commonjs",
    "jsx": "react",
    "target": "es5",
    "types": ["react", "react-dom", "react-router", "react-router-dom"],
    "typeRoots": ["./node_modules/@types"]
  },
  "include": ["src/*.ts", "src/*.tsx"],
  "exclude": ["node_modules"]
}
